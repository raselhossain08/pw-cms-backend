{
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:dev:fast": "nest start --watch --entryFile app-fast.module",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "start:prod:optimized": "NODE_ENV=production ENABLE_SWAGGER=false node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:create-indexes": "ts-node -r tsconfig-paths/register src/scripts/create-indexes.ts",
    "analyze:bundle": "nest build && source-map-explorer dist/src/main.js",
    "profile:startup": "node --prof dist/src/main.js",
    "seed": "ts-node -r tsconfig-paths/register src/scripts/seed-database.ts",
    "seed:header": "ts-node -r tsconfig-paths/register src/scripts/seed-header-cms.ts",
    "seed:footer": "ts-node -r tsconfig-paths/register src/cms/footer/scripts/seed-footer.ts",
    "seed:banners": "ts-node -r tsconfig-paths/register src/scripts/seed-banners.ts",
    "seed:banners:clear": "ts-node -r tsconfig-paths/register src/scripts/seed-banners.ts clear",
    "seed:about": "ts-node -r tsconfig-paths/register src/scripts/seed-about-section.ts",
    "seed:about-us": "ts-node -r tsconfig-paths/register src/scripts/seed-about-us.ts",
    "seed:events": "ts-node -r tsconfig-paths/register src/scripts/seed-events.ts",
    "seed:testimonials": "ts-node -r tsconfig-paths/register src/scripts/seed-testimonials.ts",
    "seed:blog": "ts-node -r tsconfig-paths/register src/cms/home/blog/scripts/seed-blog.ts",
    "seed:lessons-modules": "ts-node -r tsconfig-paths/register scripts/seed-lessons-modules.ts",
    "seed:quick-demo": "ts-node -r tsconfig-paths/register scripts/seed-quick-demo.ts",
    "seed:courses": "ts-node -r tsconfig-paths/register src/scripts/seed-courses.ts",
    "seed:categories": "ts-node -r tsconfig-paths/register src/scripts/seed-categories.ts",
    "seed:categories:direct": "ts-node src/scripts/seed-categories-direct.ts",
    "seed:products": "ts-node -r tsconfig-paths/register src/scripts/seed-products.ts",
    "seed:coupons": "ts-node -r tsconfig-paths/register src/scripts/seed-coupons.ts",
    "test:coupons": "ts-node -r tsconfig-paths/register src/scripts/test-coupon-validation.ts",
    "seed:terms-conditions": "ts-node -r tsconfig-paths/register src/cms/terms-conditions/scripts/seed-terms-conditions.ts",
    "test:footer": "ts-node -r tsconfig-paths/register src/cms/footer/scripts/test-footer.ts",
    "create-admin": "ts-node -r tsconfig-paths/register src/scripts/create-admin.ts",
    "check:websocket": "node check-websocket-setup.js"
  }
}

